{
  "name": "envase",
  "version": "0.0.0-development",
  "description": "Homebrew for docker apps",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently -k yarn:dev-electron yarn:dev-ui",
    "dev-electron": "cross-env NODE_ENV=development electron ./main.js",
    "dev-ui": "cross-env BROWSER=none react-scripts start",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:staged": "eslint --fix",
    "prettify": "prettier --write",
    "build": "yarn build-react && electron-builder --config electron.build.json",
    "build-mac": "yarn build --mac",
    "build-win": "yarn build --win",
    "build-linux": "yarn build --linux",
    "build-react": "react-scripts build"
  },
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
  "repository": "https://github.com/ameerthehacker/dockapp.git",
  "author": "ameerthehacker",
  "license": "Apache",
  "private": false,
  "devDependencies": {
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.1",
    "@types/dockerode": "^2.5.26",
    "@types/jest": "^25.1.4",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.26",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-select": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "ansi-colors": "^4.1.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.0",
    "electron-builder": "^22.5.1",
    "electron-debug": "^3.0.1",
    "emotion-theming": "^10.0.27",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.0.8",
    "lodash-es": "^4.17.15",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "resize-observer": "^1.0.0",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0",
    "xterm": "^4.5.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-unicode11": "^0.1.1",
    "xterm-addon-web-links": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,html,json,scss,md,yml,spec}": [
      "yarn prettify"
    ],
    "*.{spec,js,ts,tsx}": [
      "yarn lint:staged"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dockerode": "^3.2.0",
    "electron-store": "^5.1.1",
    "open": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
